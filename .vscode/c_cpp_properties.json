{
    "configurations": [
        {
            "name": "SGDK",
            "includePath": [
                "${env:GDK}/inc",
                "${env:GDK}/res",
                "${workspaceFolder}/inc",
                "${workspaceFolder}/res"
            ],
            "defines": [
                "__GENESIS__",
                "__SGDK__"
            ],
            "compilerPath": "${env:GDK}/bin/gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${env:GDK}/inc",
                    "${env:GDK}/res",
                    "${workspaceFolder}/inc",
                    "${workspaceFolder}/res",
                    "${workspaceFolder}/src"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}